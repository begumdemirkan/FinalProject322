@using Microsoft.AspNetCore.Identity
@inject SignInManager<Userr> SignInManager
@inject UserManager<Userr> UserManager

<ul class="navbar-nav">
    @if (SignInManager.IsSignedIn(User))
    {
    <li class="nav-item row">
        <a class="nav-link text-light" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Hoþgeldin @User.Identity.Name!</a>
        <div class="col-1"></div>
        <a class="nav-link text-light" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">Logout</a>
   
    </li>
       
        <li class="nav-item row">
            
        </li>
    }
    else
    {
        <li class="nav-item row">

            <a class="nav-link text-light" asp-area="Identity" asp-page="/Account/Login">Login</a>
            <div class="col-2"></div>
            <a class="nav-link text-light" asp-area="Identity" asp-page="/Account/Register">Register</a>
        </li>





    }
</ul>