</head>
<body class="animsition">
    <div class="page-wrapper">
        <div class="page-content--bge5">
            <div class="container">
                <div class="login-wrap">
                    <div class="login-content">



                        <div class="login-logo">
                            <a href="#">
                                <img src="~/admin/images/icon/logo.png" alt="CoolAdmin">
                            </a>


                        </div>

                        <div class="row">

                            <div class="col-12">
                                <h4 class="text-info">Yeni Üye Kayıt</h4>
                                <form asp-route-returnUrl="@Model.ReturnUrl" method="post">
                                    <div class="border border-light">
                                        <hr />
                                        <div class="form-group row">
                                            <div class="col-2">
                                                <label asp-for="Input.Ad" class="control-label"></label>
                                            </div>
                                            <div class="col-8">
                                                <input asp-for="Input.Ad" class="form-control" />
                                            </div>
                                            <span asp-validation-for="Input.Ad" class="text-danger"></span>
                                        </div>
                                        <div class="form-group row">
                                            <div class="col-2">
                                                <label asp-for="Input.Soyad" class="control-label"></label>
                                            </div>
                                            <div class="col-8">
                                                <input asp-for="Input.Soyad" class="form-control" />
                                            </div>
                                            <span asp-validation-for="Input.Soyad" class="text-danger"></span>
                                        </div>
                                        <div class="form-group row">
                                            <div class="col-2">
                                                <label asp-for="Input.Email" class="control-label"></label>
                                            </div>
                                            <div class="col-8">
                                                <input asp-for="Input.Email" class="form-control" />
                                            </div>
                                            <span asp-validation-for="Input.Email" class="text-danger"></span>
                                        </div>
                                        <div class="form-group row">
                                            <div class="col-2">
                                                <label asp-for="Input.Telefon" class="control-label"></label>
                                            </div>
                                            <div class="col-8">
                                                <input asp-for="Input.Telefon" class="form-control" />
                                            </div>
                                            <span asp-validation-for="Input.Telefon" class="text-danger"></span>
                                        </div>
                                        <div class="form-group row">
                                            <div class="col-2">
                                                <label asp-for="Input.Adres" class="control-label"></label>
                                            </div>
                                            <div class="col-8">
                                                @*<input asp-for="Input.Adres"  />*@
                                                <textarea class="form-control" rows="4" id="comment"></textarea>
                                            </div>
                                            <span asp-validation-for="Input.Adres" class="text-danger"></span>
                                        </div>

                                        <div class="form-group row">
                                            <div class="col-2">
                                                <label asp-for="Input.Password" class="control-label"></label>
                                            </div>
                                            <div class="col-8">
                                                <input asp-for="Input.Password" class="form-control" />
                                            </div>
                                            <span asp-validation-for="Input.Password" class="text-danger"></span>
                                        </div>
                                        <div class="form-group row">
                                            <div class="col-2">
                                                <label asp-for="Input.ConfirmPassword" class="control-label"></label>
                                            </div>
                                            <div class="col-8">
                                                <input asp-for="Input.ConfirmPassword" class="form-control" />
                                            </div>
                                            <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
                                        </div>

                                        <div class="form-group row">
                                            <div class="col-3">
                                                <button type="submit" class="btn btn-primary">Kayıt Ol</button>
                                            </div>
                                            <div class="col-3 ">
                                                Zaten üye misin?

                                            </div>
                                            <div class="col-3 ">
                                                <a href="~/identity/Account/Login">Giriş Yap</a>
                                            </div>
                                        </div>
                                        </div>

                                </form>







                            </div>
                       



